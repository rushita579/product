{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@api/*": ["src/api/*"],
        "@assets/*": ["src/assets/*"],
        "@common/*": ["src/common/*"],
        "@components/*": ["src/components/*"],
        "@i18n/*": ["src/i18n/*"],
        "@navigation/*": ["src/navigation/*"],
        "@redux/*": ["src/redux/*"],
        "@screen/*": ["src/screen/*"],
        "@style/*": ["src/style/*"],
        "@utils/*": ["src/utils/*"]
      }
    }
}